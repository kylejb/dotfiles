{
  "css.format.spaceAroundSelectorSeparator": true,
  "debug.console.fontSize": 16,
  "diffEditor.ignoreTrimWhitespace": true,
  "docker.extension.enableComposeLanguageServer": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "emmet.triggerExpansionOnTab": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.sortOrder": "foldersNestsFiles",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 4000,
  "files.enableTrash": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.svn": true,
    "**/.venv": true,
    "**/.DS_Store": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "**/.coverage": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autoStash": true,
  "git.branchSortOrder": "alphabetically",
  "git.enableCommitSigning": true,
  "git.repositoryScanIgnoredFolders": ["node_modules", ".venv"],
  "git-graph.dialog.general.referenceInputSpaceSubstitution": "Hyphen",
  "git-graph.repository.commits.showSignatureStatus": true,
  "git-graph.repository.fetchAndPrune": true,
  "gitlens.defaultDateFormat": "MMMM Do, YYYY HH:MM",
  "gitlens.defaultDateShortFormat": "MM-DD-YYYY",
  "gitlens.defaultTimeFormat": "HH:MM",
  "go.inferGopath": true,
  "go.toolsManagement.autoUpdate": true,
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "html.format.wrapLineLength": 100,
  "javascript.suggest.paths": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "js/ts.implicitProjectConfig.target": "ESNext",
  "markdown.preview.scrollEditorWithPreview": false,
  "material-icon-theme.folders.associations": {
    "internal": "Lib",
    "Web": "React-components"
  },
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.enableRunFromFolder": true,
  "python.analysis.typeCheckingMode": "standard",
  "python.defaultInterpreterPath": "~/.local/share/mise/shims/python",
  "python.testing.pytestEnabled": true,
  "python.testing.pytestPath": "~/.local/bin/pytest",
  "python.venvFolders": [".venv"],
  "redhat.telemetry.enabled": false,
  "remote.defaultExtensionsIfInstalledLocally": [
    "GitHub.copilot",
    "GitHub.copilot-chat",
    "GitHub.vscode-pull-request-github"
  ],
  "search.exclude": {
    "**/.venv": true
  },
  "search.showLineNumbers": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.external.windowsExec": "wt.exe",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.env.osx": {
    "PATH": ".",
    "PYTHONPATH": "."
  },
  "terminal.integrated.fontFamily": "Fira Code",
  "typescript.suggest.paths": true,
  "update.mode": "start",
  "window.restoreWindows": "none",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.empty.hint": "hidden",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "none",
  "yaml.format.singleQuote": true,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "prettier.singleQuote": true,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.renderWhitespace": "all",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
